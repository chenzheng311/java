package a;

import java.util.Scanner;

public class a {
	public static void main(String args[]) {
    //定义变量
		double money;
		int twoNumber;
		int oneNumber;
		int fiftyNumber;
		int twentyNumber;
		int tenNumber;
		int fiveNumber;
	//运算处理
		Scanner scan=new Scanner(System.in);
		System.out.println("请输入金钱的数量：");
		money=scan.nextDouble();
		twoNumber=(int) (money/2);
		oneNumber=(int) (money%2/1);
		fiftyNumber=(int) (money%2%1/0.50);
		twentyNumber=(int) (money%2%1%0.50/0.20);
		tenNumber=(int) (money%2%1%0.50%0.20/0.10);
		fiveNumber=(int) (money%2%1%0.50%0.20%0.10/0.05);
		//输出结果
		System.out.println("2元的数量："+twoNumber);
		System.out.println("1元的数量："+oneNumber);
		System.out.println("50分的数量："+fiftyNumber);
		System.out.println("20分的数量："+twentyNumber);
		System.out.println("10分的数量："+tenNumber);
		System.out.println("5分的数量："+fiveNumber);
		
	}
}
